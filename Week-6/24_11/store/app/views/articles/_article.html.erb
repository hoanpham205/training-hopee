<div id="<%= dom_id article %>" class='container'>     
    <tbody>
      <tr>
        <td class='text-center'><%= article.title%></td>
        <td class='text-center'><%= article.body%></td>
        <td class='text-center'><%= button_to "Show",article_path(article),method: :get,    class:"btn-success border-0 rounded-pill shadow px-3 py-2" %></td>
        <td class='text-center'><%= button_to "Edit",edit_article_path(article),method: :get, class:"btn-primary border-0 rounded-pill shadow px-3 py-2" %></td>
        <td class='text-center'><%= button_to "Delete",article_path(article),method: :delete, class:"btn-danger border-0 rounded-pill shadow px-3 py-2" %></td>
      </tr>
    </tbody>

</div>
